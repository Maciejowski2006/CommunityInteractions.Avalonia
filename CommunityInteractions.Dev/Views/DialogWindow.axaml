<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:vm="using:CommunityInteractions.Dev.ViewModels"
        mc:Ignorable="d" Width="400" Height="200"
        x:Class="CommunityInteractions.Dev.Views.DialogWindow"
        x:DataType="vm:DialogWindowViewModel"
        Title="DialogWindow"
        WindowStartupLocation="CenterOwner">
	<Button HorizontalAlignment="Center" VerticalAlignment="Center" Command="{Binding CloseDialogCommand}">Close</Button>
</Window>
